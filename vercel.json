{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "functions": {
    "api/index.go": {
      "includeFiles": "**"
    }
  }
}
